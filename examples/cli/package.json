{
    "name": "cli",
    "private": "true",
    "bin": "app.js",
    "version": "0.0.6-alpha.0",
    "description": "> 一套架构，多平台运行",
    "author": "stbui <stbui@stbui.com>",
    "homepage": "https://github.com/stbui/one#readme",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/stbui/one.git"
    },
    "scripts": {
        "build": "yarn run build:cjs && yarn run build:esm",
        "build:cjs": "rm -rf ./lib && tsc",
        "build:esm": "rm -rf ./esm && tsc --outDir esm --module es2015",
        "watch": "tsc --watch",
        "start": "tsc && nodemon lib/index.js"
    },
    "bugs": {
        "url": "https://github.com/stbui/one/issues"
    },
    "dependencies": {
        "@stbui/one-cli": "^0.0.6-alpha.0",
