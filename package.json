{
    "name": "@stbui/onejs",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "lerna run build",
        "build:cli": "lerna run build --scope @stbui/one-cli",
        "build:node": "lerna run build:bundle --scope node",
        "release": "yarn build && lerna publish --exact --conventional-commits",
        "start:node": "cd example/node && yarn start",
        "prettier": "prettier --config ./.prettierrc.js --write --list-different \"packages/*/src/**/*.{js,json,ts,tsx}\"",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
    },
    "engines": {
        "node": ">=6"
    },
    "workspaces": [
        "examples/*",
        "packages/*"
    ],
    "dependencies": {
        "express": "^4.17.1",
        "query-string": "^6.13.2",
        "reflect-metadata": "^0.1.13",
        "typescript": "^4.0.2"
    },
    "devDependencies": {
        "lerna": "^3.22.1",
        "prettier": "^2.0.5",
        "typescript": "^4.0.2",
        "conventional-changelog-cli": "^2.1.0"
    }
}
