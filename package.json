{
    "name": "@stbui/za-proxy",
    "version": "0.0.0",
    "private": true,
    "bin": "lib/index.js",
    "main": "lib/index",
    "module": "esm/index.js",
    "es2015": "./dist/core.esm.js",
    "typings": "./lib/index.d.ts",
    "scripts": {
        "build": "npm run build:cjs && npm run build:esm",
        "build:cjs": "rm -rf ./lib && tsc",
        "build:esm": "rm -rf ./esm && tsc --outDir esm --module es2015",
        "watch": "tsc --watch"
    },
    "engines": {
        "node": ">=6"
    },
    "dependencies": {
        "reflect-metadata": "^0.1.13",
        "compression": "^1.7.4",
        "consolidate": "^0.16.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "jaeger-client": "^3.15.0",
        "morgan": "^1.10.0",
        "opentracing": "^0.14.4",
        "serve-favicon": "^2.5.0",
        "http-proxy-middleware": "^1.0.5",
        "lowdb": "^1.0.0",
        "typescript": "^4.0.2"
    },
    "devDependencies": {
        "nodemon": "^1.11.0"
    }
}
