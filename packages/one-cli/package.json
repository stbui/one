{
    "name": "@stbui/one-cli",
    "version": "0.0.4-alpha.0",
    "bin": ".bin/index.js",
    "main": "lib/index.js",
    "module": "esm/index.js",
    "typings": "./lib/index.d.ts",
    "scripts": {
        "build": "yarn run build:cjs && yarn run build:esm",
        "build:cjs": "rm -rf ./lib && tsc",
        "build:esm": "rm -rf ./esm && tsc --outDir esm --module es2015",
        "watch": "tsc --watch",
        "start": "tsc && nodemon lib/index.js"
    },
    "engines": {
        "node": ">=6"
    },
    "dependencies": {
        "@stbui/one-common": "^0.0.4-alpha.0",
        "reflect-metadata": "^0.1.13"
    },
    "gitHead": "84aea4b3bc7c955cf04f0e5681ff813449a04a0d"
}
