{
    "name": "@stbui/one-cli",
    "version": "0.0.0",
    "bin": ".bin/index.js",
    "main": "lib/index.js",
    "module": "esm/index.js",
    "typings": "./lib/index.d.ts",
    "scripts": {
        "build": "yarn run build:cjs && yarn run build:esm",
        "build:cjs": "rm -rf ./lib && tsc",
        "build:esm": "rm -rf ./esm && tsc --outDir esm --module es2015",
        "watch": "tsc --watch",
        "start": "tsc && nodemon lib/index.js"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "engines": {
        "node": ">=6"
    },
    "dependencies": {
        "@stbui/one-common": "0.0.0",
        "reflect-metadata": "^0.1.13"
    }
}
