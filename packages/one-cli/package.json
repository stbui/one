{
  "name": "@stbui/one-cli",
  "version": "0.0.0",
  "private": true,
  "bin": ".bin/index.js",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "rm -rf ./lib && tsc",
    "build:esm": "rm -rf ./esm && tsc --outDir esm --module es2015",
    "watch": "tsc --watch",
    "start": "tsc && nodemon lib/index.js"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  }
}
